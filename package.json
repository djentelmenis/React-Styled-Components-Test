{
  "name": "react-single-page",
  "version": "1.0.0",
  "description": "A simple react single-page app with styled components and Formik for my web development course",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start-server start-app",
    "start-app": "parcel ./app/index.html",
    "start-server": "json-server --watch data/db.json",
    "build": "parcel build ./app/index.html"
  },
  "author": "djentelmenis",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "json-server": "^0.16.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "formik": "^2.1.4",
    "npm-run-all": "^4.1.5",
    "polished": "^3.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  }
}
